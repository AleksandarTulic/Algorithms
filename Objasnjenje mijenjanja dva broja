#include <iostream>
#include <vector>
#include <algorithm>
#include <stdio.h>
#include <cmath>

using namespace std;

int main()
{
    /*

    XOR ako je 0 i 0 = 0
               0 i 1 = 1
               1 i 0 = 1
               1 i 1 = 0

    int = 4 bajt
    8 bit = 1 bajt

    prvi bajt odredjuje znak broja da li ce biti poziitivno ili ne
    ako je prvi bit 0 tada je broj pozitivan a u suprotnom je broj negativan (0)

    x = 11110000 11110000 11110000 11110000

    x^=x;
    11110000 11110000 11110000 11110000
    11110000 11110000 11110000 11110000

    00000000 00000000 00000000 00000000 => x = 0
    
    ka da mijenjamo vrijednost dva broj pisemo sljedece
    x = 12;
    y = 4;
    x^=y;
    y^=x; => y=y^y^x; => y=0^x;
    x^=y;
    
    x=4 y=12


    */
    return 0;
}
